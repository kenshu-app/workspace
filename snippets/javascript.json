{
  "let": {
    "prefix": "l",
    "body": "let ${1:} = '${2:}'",
    "description": "変数定義"
  },
  "const": {
    "prefix": "c",
    "body": "const ${1:} = '${2:}'",
    "description": "定数定義"
  },
  "con": {
    "prefix": "con",
    "body": "console.log(${1:})",
    "description": "デバッグ出力"
  },
  "ready": {
    "prefix": "ready",
    "body": "$(function(){\n${1:}\n})",
    "description": "即時関数"
  },
  "jQ-Obj": {
    "prefix": "jq",
    "body": "$('${1:}').${2:}()",
    "description": "jQueryObject"
  },
  "jQ-click": {
    "prefix": "jqc",
    "body": ["$('${1:}').click(function(){", "\t$('${2:}').${3:}", "})"],
    "description": "jQueryClickEvent"
  },
  "getElementById": {
    "prefix": "getid",
    "body": "document.getElementById(${1:})",
    "description": "idDOM取得"
  },
  "querySelector": {
    "prefix": "qs",
    "body": "document.querySelector(${1:})",
    "description": "クエリセレクタ"
  },
  "function": {
    "prefix": "fn",
    "body": "function(){\n${1:}\n}",
    "description": "関数定義"
  },
  "arrowfunction": {
    "prefix": "(",
    "body": "() =>",
    "description": "アロー関数定義"
  }
}
